            </div>
            <div class="input-group flex-nowrap">
                <span class="input-group-text" id="inputGroup-sizing-default">Production year</span>
                <input type="number" class="form-control" placeholder="" aria-label="Year" aria-describedby="addon-wrapping">
            </div>
        </section>
        <br>
        <div>
            <button type="button" class="btn btn-outline-success"><a href="machinesedit.ejs" class="text-decoration-none link-success">Add Machine</a></button>
